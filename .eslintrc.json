{
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaVersion": 8
	},
	"extends": "eslint:recommended",
	"rules": {
		"semi": [
			"error",
			"always"
		],
		"no-console": "off"
	},
	"env": {
		"browser": true,
		"jquery": true,
		"node": true,
		"es6": true
	},
	"globals": {
		"ENV": "readonly"
	}
}